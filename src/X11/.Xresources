!! URxvt Appearance
URxvt.font: xft:Inconsolata:pixelsize=16:antialias=true
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 80x24
URxvt.internalBorder: 4
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: true
URxvt.scrollBar_right: true
URxvt.scrollstyle: plain
URxvt.scrollColor: #383838
URxvt.thickness: 6
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
URxvt.urgentOnBell: true
URxvt.depth: 24
URxvt.iso14755: false
URxvt.iso14755_52: false

!! URxvt extentions
URxvt.perl-ext-common: default,font-size,matcher

URxvt.font-size.step: 2
! URxvt.urlLauncher: sensible-browser
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg6

URxvt.keysym.Control-d: matcher:last
URxvt.keysym.Control-s: matcher:select

!! URxvt keybindings

URxvt.keysym.Control-Up: command:\033]720;1\007
URxvt.keysym.Control-Down: command:\033]721;1\007

URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt.keysym.Control-KP_Subtract: font-size:decrease
URxvt.keysym.Control-KP_Add: font-size:increase
URxvt.keysym.Control-KP_0: font-size:reset
URxvt.keysym.Control-KP_Enter: font-size:reset

